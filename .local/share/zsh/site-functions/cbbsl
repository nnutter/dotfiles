cbbsl() {
    if [[ -z "$1" ]]; then
        echo "Usage: cbbsl <subdirectory>"
        return 1
    fi

    declare -A aliases
    aliases[alt]="infra-dataflow-pipeline-alt"
    aliases[apps]="infra-k8s-apps"
    aliases[claude]="infra-dataflow-pipeline-claude"
    aliases[dags]="infra-data-dags"
    aliases[image]="infra-dataflow-pipeline-image"
    aliases[mds]="infra-mds"
    aliases[pipe]="infra-dataflow-pipeline"
    aliases[tf]="infra-dataflow-pipeline-tf"

    local subdir="$1"
    if [[ -n "${aliases[$subdir]}" ]]; then
        subdir="${aliases[$subdir]}"
    fi

    local target_dir="$HOME/src/github.com/cbbsl/$subdir"

    if [[ -d "$target_dir" ]]; then
        cd "$target_dir"
    else
        echo "Directory $target_dir does not exist"
        return 1
    fi
}
